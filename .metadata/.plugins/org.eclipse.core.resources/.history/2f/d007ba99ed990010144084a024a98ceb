package loops;

public class Pattern2 {
public static void hallo_rectangle(int row, int col) {
	for(int i=1;j<= row;i++) {
		for(int j=1;j<=col;j++)
		if(i==1 || i==row || j==1 || j==col) {
			System.out.println("*");
			}
		    else {
		    	System.out.println(" ");
		    	
		    }
		}
		System.out.println(" ");
	
     }
}
